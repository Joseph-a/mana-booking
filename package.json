{
	"name": "signal",
	"version": "1.0.0",
	"scripts": {
		"start": "cross-env BABEL_ENV=default SOURCEMAP=true MODE=development webpack --watch",
		"build": "cross-env BABEL_ENV=default SOURCEMAP=false MODE=production webpack"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-class-properties": "^7.4.4",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"@wordpress/babel-preset-default": "^1.3.0",
		"@wordpress/components": "^7.4.0",
		"autoprefixer": "^9.6.0",
		"babel-eslint": "^8.2.2",
		"babel-loader": "^8.0.6",
		"browser-sync": "^2.26.7",
		"browser-sync-webpack-plugin": "^2.2.2",
		"classnames": "^2.2.5",
		"cross-env": "^5.1.4",
		"css-loader": "^1.0.0",
		"eslint": "^5.16.0",
		"eslint-plugin-react": "^7.13.0",
		"eslint-plugin-react-hooks": "^1.6.0",
		"extract-text-webpack-plugin": "^4.0.0-alpha.0",
		"node-sass": "^4.12.0",
		"path": "^0.12.7",
		"postcss-loader": "^3.0.0",
		"raw-loader": "^0.5.1",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.22.1",
		"webpack": "^4.34.0",
		"webpack-cli": "^3.3.4"
	},
	"dependencies": {
		"@wordpress/editor": "^9.4.0",
		"@wordpress/i18n": "^3.5.0",
		"@wordpress/server-side-render": "^1.0.0",
		"axios": "^0.18.1",
		"jquery": "^3.4.1",
		"moment": "^2.24.0",
		"moment-locales-webpack-plugin": "^1.0.7",
		"qs": "^6.7.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-image-gallery": "^0.8.17",
		"react-render-html": "^0.6.0",
		"react-select": "^2.4.4",
		"uglifyjs-webpack-plugin": "^2.1.3"
	},
	"eslintConfig": {
		"extends": [
			"eslint:recommended",
			"plugin:react/recommended"
		],
		"plugins": [
			"react"
		],
		"parserOptions": {
			"ecmaFeatures": {
				"jsx": true
			}
		},
		"settings": {
			"react": {
				"createClass": "createReactClass",
				"pragma": "React",
				"version": "detect",
				"flowVersion": "0.53"
			},
			"propWrapperFunctions": [
				"forbidExtraProps",
				{
					"property": "freeze",
					"object": "Object"
				},
				{
					"property": "myFavoriteWrapper"
				}
			]
		},
		"rules": {
			"react/jsx-uses-react": "error",
			"react/jsx-uses-vars": "error"
		}
	},
	"license": "MIT"
}
